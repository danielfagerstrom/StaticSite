<li class="<%= if data.children.length then 'has-children' %>">
	<h5 class="file-name"><span class="icon <%= if data.id.slice(-1) is '/' then 'folder' else 'page' %>">asdf</span> <%= data.id.replace /\/$/, '' %></h5>
	<% if data.id.slice(-1) isnt '/': %><span class="date-modified">Published: <%= data.lastModified.readable() %></span><% end %>
</li>
